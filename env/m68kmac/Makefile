MAKEFILE        = Makefile
¥MondoBuild¥    = {MAKEFILE}  # Make blank to avoid rebuilds when makefile is modified

ObjDir          = :
Includes        = 

Sym-68K         = -sym on

COptions        = {Includes} {Sym-68K} -model near -opt space 


### Source Files ###

SrcFiles        =  ¶
				  mac.c #¶
				  #querycsv.c

RezFiles        =  ¶
				  mac.r ¶
				  size.r

### Object Files ###

